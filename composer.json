{
  "name": "ab-softlab/multisite-relation-site",
  "description": "development environment for ext:multisite_relation",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^8.1",
    "ab-softlab/multisite-relation": "dev-main",
    "ab-softlab/site-independent-page": "dev-main",
    "b13/bolt": "^2.3",
    "typo3/cms-backend": "^12.4",
    "typo3/cms-belog": "^12.4",
    "typo3/cms-beuser": "^12.4",
    "typo3/cms-core": "^12.4",
    "typo3/cms-filelist": "^12.4",
    "typo3/cms-fluid-styled-content": "^12.4",
    "typo3/cms-info": "^12.4",
    "typo3/cms-install": "^12.4",
    "typo3/cms-lowlevel": "^12.4",
    "typo3/cms-scheduler": "^12.4",
    "typo3/cms-seo": "^12.4",
    "typo3/cms-setup": "^12.4",
    "typo3/cms-tstemplate": "^12.4",
    "typo3/cms-workspaces": "^12.4"
  },
   "require-dev": {
    "codeception/codeception": "^5.1",
    "codeception/module-asserts": "^3.0",
    "codeception/module-webdriver": "^4.0",
    "typo3/testing-framework": "^8.2"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/*/*"
    }
  ]
}
